{
  "name": "cnpm",
  "version": "8.0.0",
  "description": "cnpm: npm client for cnpmjs.org",
  "preferGlobal": true,
  "bin": {
    "cnpm": "bin/cnpm"
  },
  "main": "bin/cnpm",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "contributor": "git-contributor",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "lint": "eslint bin/* lib test/*.test.js",
    "ci": "npm run lint && npm run test"
  },
  "dependencies": {
    "auto-correct": "^1.0.0",
    "bagpipe": "^0.3.5",
    "colors": "1.4.0",
    "commander": "~2.10.0",
    "cross-spawn": "~0.2.8",
    "debug": "^2.2.0",
    "giturl": "^1.0.0",
    "ini": "^1.3.4",
    "npm": "^6.13.4",
    "npm-request": "^1.0.0",
    "npminstall": "^6.1.0",
    "open": "^8.2.1",
    "rimraf": "^3.0.2",
    "urllib": "^2.17.0"
  },
  "devDependencies": {
    "coffee": "^3.3.0",
    "egg-bin": "^4.19.0",
    "egg-ci": "^1.19.0",
    "eslint": "^8.15.0",
    "eslint-config-egg": "^11.1.0",
    "git-contributor": "^1.0.10"
  },
  "homepage": "https://github.com/cnpm/cnpm",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/cnpm.git"
  },
  "bugs": {
    "url": "https://github.com/cnpm/cnpm/issues"
  },
  "keywords": [
    "cnpm",
    "npm",
    "npminstall"
  ],
  "publishConfig": {
    "tag": "latest"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "ci": {
    "type": "github",
    "version": "14, 16, 18",
    "os": {
      "github": "linux, windows, macos"
    }
  },
  "author": "fengmk2 <fengmk2@gmail.com> (https://github.com/fengmk2)",
  "license": "MIT"
}
